{
  "name": "iza-os-react-dashboard",
  "version": "3.0.0",
  "description": "IZA OS Autonomous Agent Dashboard - Enterprise AI Development Platform",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "snyk test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-table": "^8.10.7",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-error-boundary": "^4.0.11",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "react-use": "^17.4.0",
    "react-helmet-async": "^1.3.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "prismjs": "^1.29.0",
    "react-flow-renderer": "^10.3.17",
    "react-json-view": "^1.21.3",
    "react-copy-to-clipboard": "^5.0.5",
    "react-tooltip": "^5.21.4",
    "react-confetti": "^6.1.0",
    "react-spring": "^9.7.3",
    "use-gesture": "^10.3.0",
    "react-use-gesture": "^9.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "typescript": "^5.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^23.0.1",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/test": "^7.5.3",
    "storybook": "^7.5.3",
    "snyk": "^1.1248.0",
    "vercel": "^32.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@types/react-json-view": "^1.19.3",
    "@types/react-copy-to-clipboard": "^5.0.5",
    "@types/prismjs": "^1.26.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iza-os/react-dashboard.git"
  },
  "keywords": [
    "react",
    "typescript",
    "dashboard",
    "iza-os",
    "ai-agents",
    "enterprise",
    "autonomous",
    "glass-morphism",
    "framer-motion"
  ],
  "author": "IZA OS Development Team",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  }
}
